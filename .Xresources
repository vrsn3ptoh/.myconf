Xcursor.theme: whiteglass

URxvt.font:	xft:dejavu sans mono:pixelsize=12,xft:Awesome:pixelsize=12
!URxvt.font:    xft:monospace:size=7,style=regular 
URxvt.letterSpace:	0

Xft.autohint:	0
Xft.lcdfilter:	lcddefault
Xft.hintstyle:	hintsfull
Xft.hinting:	1
Xft.antialias:	1
Xft.rgba:	rgb

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1f21
*.color8:       #969896

! red
*.color1:       #cc6666
*.color9:       #cc6666

! green
*.color2:       #519f50
*.color10:      #519f50

! yellow
*.color3:       #f0c674
*.color11:      #f0c674

! blue
*.color4:       #81a2be
*.color12:      #81a2be

! magenta
*.color5:       #b294bb
*.color13:      #b294bb

! cyan
*.color6:       #8abeb7
*.color14:      #8abeb7

! white
*.color7:       #c5c8c6
*.color15:      #ffffff

! ROFI setup
! ------------------------------------------------------------------------------
 rofi.color-enabled: true
 rofi.color-window: #bb1d1f21, #33273238, #331e2529
 rofi.color-normal: #bb1d1f21, #ffc5c8c6, #bb1d1f21, #bb8abeb7, #ff1d1f21
 rofi.color-active: #bb1d1f21, #ff80cbc4, #bb273238, #bb1e2529, #ff80cbc4
 rofi.color-urgent: #bb1d1f21, #ffff1844, #bb273238, #bb1e2529, #ffff1844

 rofi.font:		DejaVu Sans Mono 20
 
 rofi.terminal:		urxvt
 rofi.ssh-client:	ssh
 rofi.ssh-command:	{terminal} -e {ssh-client} {host}


 URxvt.internalBorder:      5
 URxvt.externalBorder:      0

 URxvt.scrollBar:           false

 URxvt*fading:              0

 URxvt.perl-ext-common:     matcher,resize-font,foo

 URxvt.keysym.F11:          perl:fullscreen:switch

 URxvt.resize-font.smaller: C-Down
 URxvt.resize-font.bigger:  C-Up

 Xterm*saveLines:	   4096

 Xterm*scrollBar:           false

!transparancy
!URxvt*depth: 		   32
!URxvt*background:	   rgba:0000/0000/0600/c800

 URxvt.url-launcher:        /usr/bin/chromium
 URxvt.keysym.M-Delete:     perl:matcher:list
 URxvt.colorUL:             #7780a1
